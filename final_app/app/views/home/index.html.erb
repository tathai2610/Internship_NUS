
    <h1>Listing Newest Albums</h1>

    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Title</th>
        </tr>
      </thead>

      <tbody>
        <% @albums.each do |album| %>
          <tr>
            <td><%= album.id %></td>
            <td><%= album.title %></td>
          </tr>
        <% end %>
      </tbody>

    </table>
